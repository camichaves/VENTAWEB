        <addForeignKeyConstraint baseColumnNames="tarjeta_id"
                                 baseTableName="venta"
                                 constraintName="fk_venta_tarjeta_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tarjeta"/>

    </changeSet>
</databaseChangeLog>
